Description: Upstream doesn't take into account that libraries
  might be installed in /usr/lib/${DEB_HOST_MULTIARCH}. This
  patch fixes that.
Forwarded: will do
Author: Ruben Undheim <ruben.undheim@gmail.com>
Index: qrouter-1.2.29/configure
===================================================================
--- qrouter-1.2.29.orig/configure
+++ qrouter-1.2.29/configure
@@ -4462,7 +4462,7 @@ $as_echo "${WISH_EXE}" >&6; }
   else
     for libpfix in "lib64" "lib"
     do
-      libname="${TCL_EXEC_PREFIX}/${libpfix}/lib${TCL_LIB_NAME}${SHDLIB_EXT}"
+      libname="${TCL_EXEC_PREFIX}/${libpfix}/${DEB_HOST_MULTIARCH}/lib${TCL_LIB_NAME}${SHDLIB_EXT}"
       if test -r "$libname" ; then
         TCL_LIB_DIR="${TCL_EXEC_PREFIX}/${libpfix}"
         break
@@ -4494,7 +4494,7 @@ if test $usingTcl ; then
   else
     for libpfix in "lib64" "lib"
     do
-      libname="${TK_EXEC_PREFIX}/${libpfix}/lib${TK_LIB_NAME}${SHDLIB_EXT}"
+      libname="${TK_EXEC_PREFIX}/${libpfix}/${DEB_HOST_MULTIARCH}/lib${TK_LIB_NAME}${SHDLIB_EXT}"
       if test -r "$libname" ; then
         TK_LIB_DIR="${TK_EXEC_PREFIX}/${libpfix}"
         break
